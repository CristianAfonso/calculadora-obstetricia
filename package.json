{
  "name": "fetal-calculator-by-HGUGM",
  "version": "1.0.0",
  "author": "Cristian Afonso Nieto, ULPGC student",
  "//homepage": "https://cristianafonso.github.io/calculadora-obstetricia",
  "homepage": "./",
  "private": true,
  "main": "public/electron.js",
  "build":{
    "appId": "fetalBiometryCalculator",
    "productName": "Fetal Biometry Calculator HGUGM",
    "copyright": "Copyright Â© 2023 HGUGM",
    "mac": {
      "icon": "./public/images/sumadetodos.icns",     
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./public/images/sumadetodos.ico"   
    },
    "files": [
      "./build/**/*",
      "./dist/**/*",
      "./node_modules/**/*",
      "./public/**/*",       
      "*.js"
    ],
    "directories": {
      "buildResources": "public/images" 
    }
  },
  "dependencies": {
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.0-alpha3",
    "date-fns": "^2.30.0",
    "electron-is-dev": "^2.0.0",
    "flag-icon-css": "^4.1.7",
    "highcharts-react-official": "^3.2.0",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "js-cookie": "^3.0.5",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/bootstrap": "^5.2.6",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^25.3.1",
    "electron-builder": "^24.4.0",
    "gh-pages": "^5.0.0",
    "wait-on": "^7.0.1"
  }
}
